{
  "name": "RNTests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/?(*.)(spec|test).js"
    ]
  },
  "devDependencies": {
    "babel-jest": "^22.0.3",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.3",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.1.3"
  }
}
